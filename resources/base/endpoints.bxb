    }
    action-endpoint (TelPunchOut) {
      accepted-inputs (DutyTel1)
      local-endpoint (PunchOut.js::TelPunchOut)
    }
        action-endpoint (InfoAction) {
      accepted-inputs (Info)
      local-endpoint (infoview.js)
    }
  }

}
