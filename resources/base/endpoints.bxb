endpoints {
  action-endpoints {
    action-endpoint (GetCurrentPosition) {
      accepted-inputs (point)
      local-endpoint (getCurrentPosition.js)
    }
    action-endpoint (ShowHospitalList) {
      accepted-inputs (locationName,HospitalDiv,myPosition,Near)
      local-endpoint (showHospitalList.js)
    }
    
    action-endpoint (MapPunchOut) {
      accepted-inputs (HospitalPoint, HospitalAddr)
